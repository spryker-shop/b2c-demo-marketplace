<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="FileUpload">
        <property name="idFileUpload" type="int" strict="true"/>
        <property name="s3Url" type="string" strict="true"/>
        <property name="cdnUrl" type="string" strict="true"/>
        <property name="fileName" type="string" strict="true"/>
        <property name="originalFileName" type="string" strict="true"/>
        <property name="contentType" type="string" strict="true"/>
        <property name="fkMerchant" type="int" strict="true"/>
        <property name="fkUser" type="int" strict="true"/>
        <property name="objectType" type="string" strict="true"/>
        <property name="createdAt" type="string"/>
        <property name="user" type="User" strict="true"/>
        <property name="merchant" type="Merchant" strict="true"/>
    </transfer>

    <transfer name="FileUploadCriteria" strict="true">
        <property name="idFileUpload" type="int" strict="true"/>
        <property name="idMerchant" type="int"/>
        <property name="idUser" type="int"/>
        <property name="searchTerm" type="string" strict="true"/>
        <property name="page" type="int"/>
        <property name="pageSize" type="int"/>
        <property name="orderBy" type="string"/>
        <property name="orderDirection" type="string"/>
        <property name="filterStatus" type="string"/>
        <property name="withUser" type="bool"/>
        <property name="withMerchant" type="bool"/>
    </transfer>

    <transfer name="FileUploadCollection" strict="true">
        <property name="fileUploads" type="FileUpload[]" singular="fileUpload"/>
        <property name="pagination" type="Pagination"/>
    </transfer>

</transfers>
